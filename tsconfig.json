{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,

    "outDir": "lib",
    "rootDir": "src"
  },
  "exclude": [
    "lib",
    "node_modules"
  ],
  "include": [
    "src"
  ]
}
